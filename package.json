{
	"name": "jackgledhill.com",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview --host 0.0.0.0",
		"prepare": "svelte-kit sync || echo ''",
