		"@fortawesome/free-solid-svg-icons": "^6.7.2",
		"@fortawesome/svelte-fontawesome": "^0.2.3",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.20.3",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.9",
