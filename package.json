		"svelte": "^5.25.6",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.1.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.29.0",
		"vite": "^6.2.4"
	}
